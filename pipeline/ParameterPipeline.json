{
	"name": "ParameterPipeline",
	"properties": {
		"activities": [
			{
				"name": "PP Copy",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "0.12:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "DelimitedTextSource",
						"storeSettings": {
							"type": "AzureBlobFSReadSettings",
							"recursive": true,
							"enablePartitionDiscovery": false
						},
						"formatSettings": {
							"type": "DelimitedTextReadSettings"
						}
					},
					"enableStaging": false
				},
				"inputs": [
					{
						"referenceName": "SourcePP_DS",
						"type": "DatasetReference",
						"parameters": {
							"DSContainer": {
								"value": "@pipeline().parameters.PPContainer",
								"type": "Expression"
							},
							"DSInput": {
								"value": "@pipeline().parameters.PPInputDir",
								"type": "Expression"
							},
							"DSFilename": {
								"value": "@pipeline().parameters.PPFilename",
								"type": "Expression"
							}
						}
					}
				]
			}
		],
		"parameters": {
			"PPContainer": {
				"type": "string"
			},
			"PPInputDir": {
				"type": "string"
			},
			"PPOutputDir": {
				"type": "string"
			},
			"PPFilename": {
				"type": "string"
			}
		},
		"annotations": []
	}
}